    "/v1/airports": {
      "get": {
        "description": "Get all airports. We do not recommend using this route without any search query. This route has over 20k records",
        "operationId": "AirportsController_getAirports_v1",
        "parameters": [
          {
            "name": "search",
            "required": false,
            "in": "query",
            "description": "Search query for airport. Can be iata, ident(icao), name or a part of any these. Searching is case insensitive",
            "schema": {
              "example": "VIDP",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/AirportEntity"
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorEntity"
                }
              }
            }
          }
        },
        "security": [
          {
            "bearer": []
          }
        ],
        "summary": "Get all airports",
        "tags": [
          "Airports"
        ]
      }
    },
