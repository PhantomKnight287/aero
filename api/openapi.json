                }
              }
            }
          }
        },
        "security": [
          {
            "JWT-auth": []
          }
        ],
        "summary": "Get flights between two airports",
        "tags": [
          "Flights"
        ]
      }
    },
    "/v1/flights/bounds": {
      "get": {
        "description": "Get all aircraft currently flying within a specified geographic bounding box using FlightAware data with comprehensive aircraft details",
        "operationId": "FlightsController_getFlightsInBounds_v1",
        "parameters": [
          {
            "name": "minLat",
            "required": true,
            "in": "query",
            "description": "Minimum latitude of the bounding box",
            "schema": {
              "minimum": -90,
              "maximum": 90,
              "example": 40,
              "type": "number"
            }
          },
          {
            "name": "maxLat",
            "required": true,
            "in": "query",
            "description": "Maximum latitude of the bounding box",
            "schema": {
              "minimum": -90,
              "maximum": 90,
              "example": 50,
              "type": "number"
            }
          },
          {
            "name": "minLng",
            "required": true,
            "in": "query",
            "description": "Minimum longitude of the bounding box",
            "schema": {
              "minimum": -180,
              "maximum": 180,
              "example": -10,
              "type": "number"
            }
          },
          {
            "name": "maxLng",
            "required": true,
            "in": "query",
            "description": "Maximum longitude of the bounding box",
            "schema": {
              "minimum": -180,
              "maximum": 180,
              "example": 10,
              "type": "number"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of flights within the specified bounds with detailed aircraft information",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "timestamp": {
                      "type": "string",
                      "format": "date-time"
                    },
                    "bounds": {
                      "type": "object",
                      "properties": {
                        "minLat": {
                          "type": "number"
                        },
                        "maxLat": {
                          "type": "number"
                        },
                        "minLng": {
                          "type": "number"
                        },
                        "maxLng": {
                          "type": "number"
                        }
                      }
                    },
                    "totalAircraft": {
                      "type": "number"
                    },
                    "airborneAircraft": {
                      "type": "number"
                    },
                    "flights": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "ident_icao": {
                            "type": "string",
                            "nullable": true
                          },
                          "ident_iata": {
                            "type": "string",
                            "nullable": true
                          },
                          "callsign": {
                            "type": "string",
                            "nullable": true
                          },
                          "latitude": {
                            "type": "number",
                            "nullable": true
                          },
                          "longitude": {
                            "type": "number",
                            "nullable": true
                          },
                          "altitude": {
                            "type": "number",
                            "nullable": true
                          },
                          "velocity": {
                            "type": "number",
                            "nullable": true
                          },
                          "heading": {
                            "type": "number",
                            "nullable": true
                          },
                          "verticalRate": {
                            "type": "number",
                            "nullable": true
                          },
                          "lastContact": {
                            "type": "string",
                            "format": "date-time"
                          },
                          "isOnGround": {
                            "type": "boolean"
                          },
                          "aircraftType": {
                            "type": "string",
                            "nullable": true
                          },
                          "aircraftTypeFaa": {
                            "type": "string",
                            "nullable": true
                          },
                          "aircraftFamily": {
                            "type": "string",
                            "nullable": true
                          },
                          "manufacturer": {
                            "type": "string",
                            "nullable": true
                          },
                          "model": {
                            "type": "string",
                            "nullable": true
                          },
                          "engineType": {
                            "type": "string",
                            "nullable": true
                          },
                          "engineCount": {
                            "type": "string",
                            "nullable": true
                          },
                          "wakeTurbulenceCategory": {
                            "type": "string",
                            "nullable": true
                          },
                          "aircraftId": {
                            "type": "string",
                            "nullable": true
                          },
                          "aircraftRegistration": {
                            "type": "string",
                            "nullable": true
                          },
                          "ownerIcao": {
                            "type": "string",
                            "nullable": true
                          },
                          "ownerIata": {
                            "type": "string",
                            "nullable": true
                          },
                          "ownerName": {
                            "type": "string",
                            "nullable": true
                          },
                          "operator": {
                            "type": "string",
                            "nullable": true
                          },
                          "operatorIcao": {
