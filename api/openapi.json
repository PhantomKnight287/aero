    "/v1/flights": {
      "get": {
        "description": "Get all flights between two airports for a specific date",
        "operationId": "FlightsController_getFlights_v1",
        "parameters": [
          {
            "name": "from",
            "required": true,
            "in": "query",
            "description": "Departure airport IATA code",
            "schema": {
              "example": "LAX",
              "type": "string"
            }
          },
          {
            "name": "to",
            "required": true,
            "in": "query",
            "description": "Arrival airport IATA code",
            "schema": {
              "example": "JFK",
              "type": "string"
            }
          },
          {
            "name": "date",
            "required": true,
            "in": "query",
            "description": "Flight date in ISO format",
            "schema": {
              "example": "2024-03-20",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "",
